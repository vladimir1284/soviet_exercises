{
  "name": "flexfit",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "wrangler pages dev .svelte-kit/cloudflare",
    "deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
    "db:migrate": "wrangler d1 execute flexfit-db --file=./schema.sql",
    "db:migrate:local": "wrangler d1 execute flexfit-db --local --file=./schema.sql"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^4.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "svelte": "^4.2.8",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "wrangler": "^4.59.2"
  },
  "dependencies": {
    "@clerk/clerk-js": "^5.0.0",
    "svelte-i18n": "^4.0.0"
  }
}
